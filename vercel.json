    "installCommand": "npm install",
    "buildCommand": "composer install --no-dev --prefer-dist --optimize-autoloader && vite build && php artisan config:cache && php artisan route:cache && php artisan view:cache",
    "functions": {
        "api/index.php": { 
            "runtime": "vercel-php@0.7.4",
            "memory": 1024
        }
    },
    "routes": [
